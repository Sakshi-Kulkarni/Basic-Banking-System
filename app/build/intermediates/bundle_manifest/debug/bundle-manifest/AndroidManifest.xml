<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.saurabhlande.bankingsystem"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="30" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@drawable/bank"
        android:label="@string/app_name"
        android:roundIcon="@drawable/bank"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme_white" >
        <activity
            android:name="com.saurabhlande.bankingsystem.splash_screen"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.saurabhlande.bankingsystem.MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name="com.saurabhlande.bankingsystem.Userlist"
            android:label="Banking System"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name="com.saurabhlande.bankingsystem.Userdata"
            android:label="User Details"
            android:parentActivityName="com.saurabhlande.bankingsystem.Userlist"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.saurabhlande.bankingsystem.SendToUser"
            android:label="Transfer Money"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.saurabhlande.bankingsystem.HistoryList"
            android:label="Transactions"
            android:parentActivityName="com.saurabhlande.bankingsystem.Userlist"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
    </application>

</manifest>